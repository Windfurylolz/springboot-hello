#!groovy
properties([disableConcurrentBuilds()])

pipeline {
    agent {
       label 'master'
       }
     options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
     }
     stages {
        stage("Creating docker image") {
           steps {
              echo "buidling image"
              dir ('docker/builds') {
                  sh 'docker build . '
              }
           }
        }
     }
  }
